<tryton>
    <data>
        <record model="ir.ui.view" id="incoterm_view_form">
            <field name="model">incoterm.incoterm</field>
            <field name="type">form</field>
            <field name="name">incoterm_form</field>
        </record>

        <record model="ir.ui.view" id="incoterm_view_list">
