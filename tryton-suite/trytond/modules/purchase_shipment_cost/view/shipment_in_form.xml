<data>
    <xpath expr="//field[@name='warehouse']" position="after">
        <label name="carrier"/>
        <field name="carrier"/>
    </xpath>
    <xpath expr="//page[@id='other']" position="before">
        <page id="costs" string="Costs">
            <label name="cost_used"/>
